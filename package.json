{
  "name": "render-ui",
  "version": "0.0.2",
  "description": "React UI library with highly modular and ready-out-of-the-box components",
  "homepage": "https://github.com/lovrozagar/render-ui",
  "repository": {
    "type": "git",
    "url": "git@github.com:lovrozagar/render-ui.git"
  },
  "license": "MIT",
  "author": "Lovro Å½agar",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production BABEL_ENV=production rollup -c",
    "ci": "bun lint && bun test --watch=false",
    "cn-rws": "node ./scripts/classname-remove-white-space.js",
    "dev": "vite",
    "format": "prettier --write .",
    "preinstall": "npx only-allow bun",
    "lint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "lint:fix": "eslint . --ext .js,.jsx,.tsx,.ts --fix",
    "prepare": "husky install",
    "release": "bun ci && bun build && npm publish",
    "start": "NODE_ENV=development BABEL_ENV=development bun storybook",
    "storybook": "storybook dev -p 6006 -s src",
    "storybook-build": "storybook build",
    "storybook:build": "storybook build",
    "storybook:deploy": "bun storybook:build && touch ./storybook-static/.nojekyll && gh-pages -d ./storybook-static -t true",
    "test": "vitest --config ./vitest.config.ts",
    "type-check": "tsc --pretty --noEmit"
  },
